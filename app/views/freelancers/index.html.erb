<% @freelancers.each do |f| %>

	<div class="card my-2" style="width: 18rem;">
  <img src="..." class="card-img-top" alt="...">
  <div class="card-body">
    <h3 class="card-title"><%= link_to f.name,root_path %></h3>
    <h4>email : <%= f.email%></h4>
    <% if current_user!=nil or current_freelancer!=nil or current_admin!=nil%>
      <a href="#" class="btn btn-primary">Send Connection Request</a>
    <% end %>
    <% if current_admin!=nil%>
      <a href="#" class="btn btn-warning">Edit Credential</a>
      <a href="#" class="btn btn-danger">Delete Credential</a>
    <% end %>
  </div>
</div>
<% end %>
